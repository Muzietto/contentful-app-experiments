{
  "name": "js-features-manager",
  "version": "0.1.1",
  "description": "app embryo for Contentful-based JSON editing",
  "private": true,
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testmocha": "mocha 'src/**/*.spec.js'",
    "testwatchmocha": "mocha --watch",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@contentful/app-sdk": "^4.21.1",
    "@contentful/f36-components": "4.44.0",
    "@contentful/f36-tokens": "4.0.2",
    "@contentful/field-editor-json": "^3.1.3",
    "@contentful/react-apps-toolkit": "1.2.16",
    "contentful-management": "10.38.2",
    "@reduxjs/toolkit": "^1.9.0",
    "emotion": "10.0.27",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "paco-js": "^1.0.3"
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.10.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "cross-env": "7.0.3",
    "esbuild": "^0.18.10",
    "@babel/register": "^7.9.0",
    "mocha": "latest",
    "chai": "latest"
  },
  "homepage": "."
}
